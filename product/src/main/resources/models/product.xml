<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fh.dao.ProductDao">
	<insert id="addProduct" parameterType="com.fh.model.Product">
		insert into t_product (productname,price,pinPai) values(#{productname},#{price},#{pinPai})
	</insert>
	<select id="pinpaiqueryList" resultType="com.fh.model.PinPai">
		select pid,pname from t_pinpai 
	</select>
</mapper>